 | collisions                   # [R] Eval the extended Collisions
 | BoseTemp= 0.000000     eV    # Bosonic Temperature
 | DBsIOoff= "DIP COLLs"        # [IO] Space-separated list of DB with NO I/O. DB=(DIP,X,HF,COLLs,J,GF,CARRIERs,W,SC,BS,ALL)
 | HXC_Potential= "HARTREE+FOCK" # [SC] SC HXC Potential
 | FFTGvecs=  3317        RL    # [FFT] Plane-waves
 | % SCBands
 |   1 | 10 |                   # [SC] Bands
 | %
 | % COLLBands
 |   1 | 10 |                   # [COLL] Bands for the collisions
 | %
 | EXXRLvcs=  3413        RL    # [XX] Exchange RL components
 | ALLGexx                      # [XX] Force the use use all RL vectors for the exchange part
 | HARRLvcs=  3413        RL    # [HA] Hartree  RL components
 | SCIter= 3                    # [SC] SC Iterations
 | SCEtresh=   0.0100     eV    # [SC] Energy convergence threshold
 | SCRhoTresh=0.1000E-4         # [SC] Rho convergence threshold
