 | electrons                        # [R] Electronic properties
 | dos                              # [R] DOS
 | bnds                             # [R] Bands
 | ElecTemp= 0.025867         eV    # Electronic Temperature
 | BoseTemp= 0.025850         eV    # Bosonic Temperature
 | INTERP_mode= "NN"                # Interpolation mode (NN=nearest point, BOLTZ=boltztrap aproach)
 | OutputAlat= 0.000000             # [a.u.] Lattice constant used for "alat" ouput format
 | cooIn= "rlu"                     # Points coordinates (in) cc/rlu/iku/alat
 | cooOut= "rlu"                    # Points coordinates (out) cc/rlu/iku/alat
 | % BANDS_bands
 |   1 |  5 |                           # Number of bands
 | %
 | INTERP_Shell_Fac= 20.00000       # The bigger it is a higher number of shells is used
 | INTERP_NofNN= 1                  # Number of Nearest sites in the NN method
 | CIRCUIT_E_DB_path= "PW_SAVE"     # SAVE obtained from the QE `bands` run (alternative to %BANDS_kpts)
 | BANDS_path= " Kp M G Kp A L G"   # High-Symmetry points labels (G,M,K,L...) also using composed positions (0.5xY+0.5xL).
 | BANDS_steps=  50                 # Number of divisions
 | #BANDS_built_in                # Print the bands of the generating points of the circuit using the nearest internal point
 | GfnQPdb= "E < QP/ndb.QP_merged_1_gw_real_axis" # [EXTQP G] Database action
 | PROJECT_mode= "none"             # Instruct ypp how to project the DOS. ATOM, LINE, PLANE.
 | PROJECT_path= "none"             # Project path. PLANE mode: 3 points in the plane. LINE mode: any series of points.
 | PROJECT_steps=-1                 # Project LINE/PLANE-axis steps.
 | PROJECT_depth=-1                 # Project perpendicular direction divisions.
