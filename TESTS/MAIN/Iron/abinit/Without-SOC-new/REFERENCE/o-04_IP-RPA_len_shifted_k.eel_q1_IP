# | bse                              # [R][BSE] Bethe Salpeter Equation.
# | BoseTemp= 0.000000         eV    # Bosonic Temperature
# | FFTGvecs= 1721             RL    # [FFT] Plane-waves
# | BS_nCPU_LinAlg_INV= 1            # [PARALLEL] CPUs for Linear Algebra (if -1 it is automatically set)
# | BS_nCPU_LinAlg_DIAGO= 1          # [PARALLEL] CPUs for Linear Algebra (if -1 it is automatically set)
# | X_Threads= 1                     # [OPENMP/X] Number of threads for response functions
# | DIP_Threads= 1                   # [OPENMP/X] Number of threads for dipoles
# | K_Threads= 1                     # [OPENMP/BSK] Number of threads for response functions
# | BSKmod= "IP"                     # [BSE] IP/Hartree/HF/ALDA/SEX/BSfxc
# | BSEmod= "retarded"               # [BSE] resonant/retarded/coupling
# | DipApproach= "Shifted grids"     # [DIP] [G-space v/R-space x/Covariant/Shifted grids]
# | DipComputed= "R V P"             # [DIP] [default R P V; extra P2 Spin Orb]
# | ShiftedPaths= "SHIFTED_grids/shift_1 SHIFTED_grids/shift_2 SHIFTED_grids/shift_3" # [DIP] Shifted grids paths (separated by a space)
# | BSENGexx= 10833            RL    # [BSK] Exchange components
# | DrudeWBS= ( 4.800000 , 5.650000 )  eV    # [BSE] Drude plasmon
# | % BSEQptR
# |  1 | 1 |                             # [BSK] Transferred momenta range
# | %
# | % BSEBands
# |   1 | 13 |                           # [BSK] Bands range
# | %
# | % BEnRange
# |  0.200000 | 8.000000 |         eV    # [BSS] Energy range
# | %
# | % BDmRange
# |  0.300000 | 0.500000 |         eV    # [BSS] Damping range
# | %
# | BEnSteps=  400                   # [BSS] Energy steps
# | % BLongDir
# |  0.100000E-4 |  0.00000    |  0.00000    # [BSS] [cc] Electric Field
# | %
# | BSEprop= "abs jdos"              # [BSS] Can be any among abs/jdos/kerr/magn/dich/photolum/esrt
# | BSEdips= "none"                  # [BSS] Can be "trace/none" or "xy/xz/yz" to define off-diagonal rotation plane
