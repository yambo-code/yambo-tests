 | HXC_Potential= "HARTREE+COH+SEX" # [SC] SC HXC Potential
 | HARRLvcs= 2227             RL    # [HA] Hartree     RL components
 | EXXRLvcs=  3413            RL    # [XX] Exchange    RL components
 | CORRLvcs=  51              RL    # [GW] Correlation RL components
