wavefunction                 # [R] Wavefunction
electrons                    # [R] Electrons (and holes)
Format= "g"                  # Output format [(c)ube/(g)nuplot/(x)crysden]
Direction= "12"              # [rlu] [1/2/3] for 1d or [12/13/23] for 2d [123] for 3D
