
from yambopy import *

ns_db1='SAVE/ns.db1'

ylat = YamboLatticeDB.from_db_file(filename=ns_db1)

out = open('01_init/o-01_init_std.yambopy.out','w')
out.write(ylat.__str__()+'\n')
out.write('# Additional data\n')
np.savetxt(out,ylat.car_kpoints,header='kpoints c.c.')
out.write('# symmetries c.c.\n')
for sym in ylat.sym_car: np.savetxt(out,sym)
out.close()
