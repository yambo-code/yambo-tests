 .-Input file  yambo.in
 | negf                             # [R] NEQ Real-time dynamics
 | RT_Threads= 1                    # [OPENMP/RT] Number of threads for real-time
 | HXC_Potential= "IP"              # [SC] SC HXC Potential
 | FFTGvecs=  645             RL    # [FFT] Plane-waves
 | HARRLvcs=  645             RL    # [HA] Hartree     RL components
 | RTEvalEnergy                   # [RT] Energy variation computed on the fly
 | RTEvalEntropy                  # [RT] Entropy variation computed on the fly
 | dTupdateTime= 1.000000     fs    # [RT] Initial Time for deltaT update (active only if non-zero)
 | dTupdateJump= 1.000000     fs    # [RT] Time betweem two deltaT updates
 | dTupdateTresh= 5.000000          # [RT][o/o] Treshold of deltaT updates
 | dT_MAX= 0.200000           fs    # [RT] Maximum value for the time-dependent dT
 | % RTBands
 |   2 |  5 |                           # [RT] Bands
 | %
