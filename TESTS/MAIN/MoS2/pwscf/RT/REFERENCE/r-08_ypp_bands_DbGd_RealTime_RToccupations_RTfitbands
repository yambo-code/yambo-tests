 .-Input file  yambo.in
 | RealTime                         # [R] TD Post-Processing
 | RToccupations                    # [R] Analize time-dependent occupations
 | RTfitbands                       # [R] Post-Processing kind: fit on bands
 | INTERP_mode= "NN"                # Interpolation mode (NN=nearest point, BOLTZ=boltztrap aproach)
 | OutputAlat= 11.75442             # [a.u.] Lattice constant used for "alat" ouput format
 | cooIn= "rlu"                     # Points coordinates (in) cc/rlu/iku/alat
 | cooOut= "rlu"                    # Points coordinates (out) cc/rlu/iku/alat
 | #NormN                         # Nel on the bands is renormalized to Nel in the IBZ
 | NelFac= 1.000000                 # Rescale the total number of electrons
 | INTERP_Shell_Fac= 20.00000       # The bigger it is a higher number of shells is used
 | INTERP_NofNN= 1                  # Number of Nearest sites in the NN method
 | CIRCUIT_E_DB_path= "none"        # SAVE obtained from the QE `bands` run (alternative to %BANDS_kpts)
 | BANDS_path= " G"                 # High-Symmetry points labels (G,M,K,L...) also using composed positions (0.5xY+0.5xL).
 | BANDS_steps= 30                  # Number of divisions
 | #BANDS_built_in                # Print the bands of the generating points of the circuit using the nearest internal point
 | TimeStep= 10.07994         fs    # Time step
 | % TimeRange
 |   0.00000 | 10.08000 |         fs    # Time-window where processing is done
 | %
 | #IncludeEQocc                  # [NEGF] Include also equilibrium occupations
 | #SkipFermiFIT                  # [NEGF] Do a Fermi Fit of occupations (and lifetimes ratio)
 | PROJECT_mode= "none"             # Instruct ypp how to project the DOS. ATOM, LINE, PLANE.
 | PROJECT_depth=-1                 # Project perpendicular direction divisions.
 | %BANDS_kpts                      # K points of the bands circuit
 |  0.000000| 0.000000| 0.000000|
 |  0.500000| 0.500000| 0.500000|
 | %
